include <stdio.h>
struct Address {
char city[50];
int pin;
};
struct person {
char name[50];
struct Address addr;
};
int main() {
 struct person p;
  printf ("bindhu t j:");
  scanf("%s", p.name);
  printf ("chitradurga:");
  scanf("%s", p.addr.city);
  printf ("577501:");
  scanf("%d", &p.addr.pin);
  printf ("\nDisplying information\n");
  printf("bindhu t j: %s\n", p.name);
  printf ("chitradurga: %s\n", p. addr.city);
  printf ("577501: %d\n", p.addr.pin);
  return 0;
}
