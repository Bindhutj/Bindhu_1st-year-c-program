{ int z;
   printf("Choose the unit to be converted :\n\n");
   printf("1. Celsius to Fahrenheit \n\n");
   scanf("%d", &z);
   float c, f;
if(z==1){
 printf("Enter the temperature in celcius:\n ");
    scanf("%f", &c);
